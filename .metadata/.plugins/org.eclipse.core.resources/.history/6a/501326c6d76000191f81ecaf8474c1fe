(deftemplate manusia
    (slot nama (default "-"))
    (multislot hobi (default "ga punya"))
)

(assert (budi-bermain-bola))

(assert (manusia
        (nama "budi")
        (hobi "makan" "tidur")))

(assert (manusia))


(printout t (retract 1) crlf)
(printout t (retract 1) crlf)

(modify 1 (nama "berubah"))		;retract tidak mengubah urutan nomor fact

(deffacts name
    (manusia (nama "adam") (hobi "ngewe"))
    (manusia (nama "hawa") (hobi "ngewe"))
)

(reset)
(duplicate 1 (nama "andi 2") (hobi "ngentot")) 

